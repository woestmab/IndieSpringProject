<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="dbManager" class="">
        <property name="jdbcDriver" value="com.mysql.jdbc.Driver"/>
        <property name="jdbcUrl" value="jdbc:mysql://localhost"/>
        <property name="jdbcUser" value="woestmab"/>
        <property name="jdbcPassword" value="admin"/>
    </bean>
    <bean id="appointment" class="Appointment">
        <property name="id" ></property>
    </bean>
</beans>
